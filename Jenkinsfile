pipeline {
  agent any
  
   stages {
    stage("init") {
      steps {
        echo 'init the application'
      }
    }
  stages {
    stage("build") {
      steps {
        echo 'Building the application'
      }
    }
    stage("test") {
      steps {
        echo 'Testing the application'
      }
    }
    stage("deploy") {
      steps {
        echo 'Deploying the application'
      }
    }
  }
}
